{
    "form_data_definition_version": "1",
    "form": {
        "bind_type": "child",
        "default_bind_path": "/model/instance/Child_Vitamin_A_EngKan/",
        "fields": [
            {
                "name": "id",
                "value": "<%= entity_id %>",
                "source": "child.id"
            },
            {
                "name": "dateOfBirth",
                "value": "<%= child['Birth date'] %>",
                "source": "child.dateOfBirth"
            },
            {
                "name": "vitaminADose",
                "value": "<%= vitamin_a_dosage['Vitamin A dose'] %>",
                "source": "child.vitaminADose"
            },
            {
                "name": "vitaminADate",
                "value": "<%= vitamin_a_dosage['Vitamin A date'] %>",
                "source": "child.vitaminADate"
            },
            {
                "name": "vitaminAPlace",
                "value": "<%= vitamin_a_dosage['Vitamin A Dosage Place'] %>",
                "source": "child.vitaminAPlace"
            },
            {
                "name": "submissionDate",
                "value": "<%= submission_date %>",
                "source": "child.submissionDate"
            }
        ]
    }
}